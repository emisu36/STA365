        "\n",
        "axes[1].hist(samples_theta_strong, bins=30, density=True, alpha=0.6, color='g')\n",
        "axes[1].set_title(\"Strong Prior (Alpha=10, Lambda=10)\")\n",
        "\n",
        "axes[2].hist(samples_theta_info, bins=30, density=True, alpha=0.6, color='r')\n",
        "axes[2].set_title(\"Informative Prior (Theta_0=3, Tau_0^2=0.5)\")\n",
