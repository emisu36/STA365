        "# Run with different priors\n",
        "samples_theta_weak, samples_tau_weak = gibbs_sampler_q2(x, alpha=1, lambda_=1)\n",
        "samples_theta_strong, samples_tau_strong = gibbs_sampler_q2(x, alpha=10, lambda_=10)\n",
        "samples_theta_info, samples_tau_info = gibbs_sampler_q2(x, theta_0=3, tau2_0=0.5)\n",
        "\n",
        "# Compare posteriors\n",
        "fig, axes = plt.subplots(1, 3, figsize=(15, 5))\n",
        "\n",
        "axes[0].hist(samples_theta_weak, bins=30, density=True, alpha=0.6, color='b')\n",
